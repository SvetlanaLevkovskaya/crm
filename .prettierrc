{
  "trailingComma": "es5",
  "semi": false,
  "tabWidth": 4,
  "singleQuote": true,
  "printWidth": 100,
  "bracketSameLine": false,
  "endOfLine": "auto",
  "importOrder": [
    "^react$",
    "^[a-z]",
    "^@?\\w",
    "^\\.",
    "^.*$"
  ],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}
